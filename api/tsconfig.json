{
  "compilerOptions": {
    "target": "es2017",
    "noImplicitAny": true,
    "strict": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "preserveSymlinks": true,
    "lib": [
      "es2017",
      "webworker"
    ],
    "baseUrl": "src",
    "skipLibCheck": true,
  },
}